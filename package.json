{
  "name": "redux-epics-decorator",
  "version": "0.7.2",
  "description": "Dumb decorator for redux & redux-observable & react-redux & redux-actions",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "check_circular_dependencies": "madge lib/index.js --circular --warning",
    "compile": "rm -rf lib && tsc -p tsconfig.json --diagnostics --pretty",
    "lint": "tslint -c tslint.json -p tsconfig.json && tslint -c tslint.json -p test/tsconfig.json",
    "precommit": "lint-staged",
    "start": "webpack-dev-server --config ./webpack.config.js --progress --color",
    "test": "rm -rf coverage && NODE_ENV=test karma start",
    "watch": "karma start",
    "publish_all": "ts-node ./tools/publish.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Brooooooklyn/redux-epics-decorator.git"
  },
  "keywords": [
    "Redux",
    "redux-observable",
    "rxjs",
    "TypeScript",
    "reflect-metadata"
  ],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brooooooklyn/redux-effects-decorator/issues"
  },
  "homepage": "https://github.com/Brooooooklyn/redux-effects-decorator#readme",
  "dependencies": {
    "injection-js": "^2.2.1",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.9.2"
  },
  "peerDependencies": {
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/enzyme": "^3.1.10",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.2",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "^4.0.26",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-actions": "^2.3.0",
    "@types/shelljs": "^0.8.0",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "injection-js": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^3.0.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "madge": "^3.1.1",
    "mocha": "^5.2.0",
    "prettier": "^1.13.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-observable": "1.0.0",
    "rxjs": "^6.2.1",
    "shelljs": "^0.8.2",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "symbol-observable": "^1.2.0",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint -c tslint.json --fix",
      "git add"
    ]
  }
}
