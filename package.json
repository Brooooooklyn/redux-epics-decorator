{
  "name": "redux-epics-decorator",
  "version": "0.7.7",
  "description": "Dumb decorator for redux & redux-observable & react-redux & redux-actions",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "check_circular_dependencies": "madge lib/index.js --circular --warning",
    "compile": "yarn compile:es5 && yarn compile:module",
    "compile:es5": "rm -rf lib && tsc -p tsconfig.json --diagnostics --pretty",
    "compile:module": "rm -rf es && tsc -p tsconfig.json -m esnext --diagnostics --pretty --outDir es",
    "lint": "tslint -c tslint.json -p tsconfig.json && tslint -c tslint.json -p test/tsconfig.json",
    "start": "webpack-dev-server --config ./webpack.config.js --progress --color",
    "test": "rm -rf coverage && NODE_ENV=test karma start",
    "watch": "karma start",
    "publish_all": "ts-node ./tools/publish.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Brooooooklyn/redux-epics-decorator.git"
  },
  "keywords": [
    "Redux",
    "redux-observable",
    "rxjs",
    "TypeScript",
    "reflect-metadata"
  ],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brooooooklyn/redux-effects-decorator/issues"
  },
  "homepage": "https://github.com/Brooooooklyn/redux-effects-decorator#readme",
  "dependencies": {
    "injection-js": "^2.2.1",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.1.15",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^5.2.5",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.4.1",
    "@types/redux-actions": "^2.3.1",
    "@types/shelljs": "^0.8.0",
    "@types/sinon": "^5.0.6",
    "@types/sinon-chai": "^3.2.1",
    "chai": "^4.2.0",
    "connected-react-router": "^5.0.1",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "injection-js": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^8.0.5",
    "lodash": "^4.17.11",
    "madge": "^3.3.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-observable": "1.0.0",
    "rxjs": "^6.3.3",
    "shelljs": "^0.8.3",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "symbol-observable": "^1.2.0",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
