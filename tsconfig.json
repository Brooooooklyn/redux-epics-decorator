{
  "compilerOptions": {
    "strict": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "sourceMap": true,
    "declaration": true,
    "skipLibCheck": true,
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.symbol",
      "es2015.collection",
      "es2015.core"
    ],
    "outDir": "./lib"
  },
  "include": [
    "src"
  ]
}
